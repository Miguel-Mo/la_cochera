<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<Scene stylesheets="@/css/panel.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Cochera.Controllers.ControladorPanel">
   <!-- El prefHeight y prefWidth tiene que coincidir con las mismas que el fx:id="parent" de login.fxml -->
   <BorderPane fx:id="parent" prefHeight="510.0" prefWidth="842.0">
      <center>
         <Pane fx:id="root" BorderPane.alignment="CENTER" />
      </center>
      <top>
         <BorderPane prefHeight="85.0" prefWidth="878.0" BorderPane.alignment="CENTER" styleClass="top-bar">
            <right>
               <Button fx:id="close" styleClass="close" mnemonicParsing="false" onAction="#cerrar" prefHeight="60.0" prefWidth="77.0" BorderPane.alignment="CENTER" />
            </right>
            <left>
               <ImageView fitHeight="72.0" fitWidth="87.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
                  <image>
                     <Image url="@icons/astronauta.png" />
                  </image>
               </ImageView>
            </left>
         </BorderPane>
      </top>
   </BorderPane>
</Scene>
