<?xml version="1.0" encoding="UTF-8"?>

<?import Cochera.utils.vistas.Boton?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import org.controlsfx.control.ToggleSwitch?>
<AnchorPane id="root_pane" prefHeight="352.0" prefWidth="501.0" stylesheets="@/css/formModales.css"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">

    <Pane id="pane_cont" prefHeight="352.0" prefWidth="501.0" AnchorPane.bottomAnchor="0.0"
          AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">

        <Label layoutX="50.0" layoutY="37.0" text="Marca"/>
        <TextField fx:id="marcaVehiculo" layoutX="50.0" layoutY="55.0" prefHeight="25.0" prefWidth="174.0"/>

        <Label layoutX="274.0" layoutY="37.0" text="Modelo Vehículo"/>
        <TextField fx:id="modeloVehiculo" layoutX="274.0" layoutY="55.0" prefHeight="25.0" prefWidth="174.0"/>

        <Label layoutX="50.0" layoutY="92.0" text="Potencia"/>
        <TextField fx:id="potencia" layoutX="50.0" layoutY="110.0" prefHeight="25.0" prefWidth="174.0"/>

        <Label layoutX="274.0" layoutY="91.0" text="2ª mano"/>
        <ToggleSwitch fx:id="tswitch" layoutX="253.0" layoutY="113.0"/>

        <Label fx:id="lantiguedad" layoutX="355.0" layoutY="92.0" text="Antigüedad"/>
        <TextField fx:id="antiguedad" layoutX="355.0" layoutY="111.0" prefHeight="25.0" prefWidth="93.0"/>

        <Label layoutX="50.0" layoutY="144.0" text="Tipo vehiculo"/>
        <ComboBox fx:id="tipoVehiculo" layoutX="50.0" layoutY="162.0" prefWidth="174.0"/>

        <Label layoutX="274.0" layoutY="144.0" text="Concesionario Resgistro"/>
        <ComboBox fx:id="concesionarioRegistro" layoutX="274.0" layoutY="162.0" prefWidth="174.0"/>

        <Label layoutX="50.0" layoutY="202.0" text="Precio de compra"/>
        <TextField fx:id="precio" layoutX="50.0" layoutY="220.0" prefWidth="116.0"/>

        <Label layoutX="137.0" layoutY="202.0" text="Combustible"/>
        <TextField fx:id="combustible" layoutX="50.0" layoutY="220.0" prefWidth="116.0"/>

        <Label layoutX="274.0" layoutY="202.0" text="Km recorridos"/>
        <TextField fx:id="kmRecorridos" layoutX="50.0" layoutY="220.0" prefWidth="116.0"/>


        <Boton fx:id="btnAccion" layoutX="295.0" layoutY="288.0" mnemonicParsing="false"
                prefHeight="37.0" prefWidth="104.0" text="Editar"/>
        <Boton fx:id="btnCancelar" layoutX="81.0" layoutY="288.0" mnemonicParsing="false" prefHeight="37.0"
                prefWidth="104.0" text="Cancelar"/>
    </Pane>
</AnchorPane>
